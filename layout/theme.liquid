    <script>
          // Optimized Video Autoplay - Only play when visible to save bandwidth/battery
          document.addEventListener("DOMContentLoaded", function() {
      var lazyVideos = [].slice.call(document.querySelectorAll("video.js-video-autoplay, video[data-autoplay-on-scroll]"));

      if ("IntersectionObserver" in window) {
        var lazyVideoObserver = new IntersectionObserver(function(entries, observer) {
          entries.forEach(function(entry) {
            if (entry.isIntersecting) {
              if (entry.target.paused) {
                  entry.target.play().catch(function(e) { /* console.log('Autoplay prevented:', e); */ });
              }
            } else {
               if (!entry.target.paused) {
                  entry.target.pause();
               }
            }
          });
        }, { threshold: 0.25 }); // Play when 25% visible

        lazyVideos.forEach(function(lazyVideo) {
          lazyVideoObserver.observe(lazyVideo);
        });
      }
          });
    </script>
