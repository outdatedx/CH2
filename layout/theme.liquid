        // this is to override protection of fetch by another script
        (function() {
          const originalFetch = window.fetch; // Save your own copy

          try {
            // Attempt to make window.fetch non-writable and non-configurable
            // This only works if no other script has already done so or overridden it.
            Object.defineProperty(window, 'fetch', {
              value: originalFetch,
              writable: false,
              configurable: false
            });
          // console.log("Attempted to protect window.fetch from global overrides.");
