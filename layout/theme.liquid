    <head>
    <script>
        // Defer Microsoft Clarity until after page is interactive for better FCP
        function loadClarity() {
          (function(c,l,a,r,i,t,y){
      c[a]=c[a]||function(){(c[a].q=c[a].q||[]).push(arguments)};
      t=l.createElement(r);t.async=1;t.src="https://www.clarity.ms/tag/"+i;
      y=l.getElementsByTagName(r)[0];y.parentNode.insertBefore(t,y);
          })(window, document, "clarity", "script", "smy68h3x4o");
        }
        if ('requestIdleCallback' in window) {
          requestIdleCallback(function() { loadClarity(); }, { timeout: 3000 });
        } else {
          window.addEventListener('load', function() { setTimeout(loadClarity, 1000); });
        }
    </script>


